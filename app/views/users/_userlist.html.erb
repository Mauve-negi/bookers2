<h2>Users</h2>

<table class="table table-hover table-inverse">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% ulist.each do |ulist| %>
      <tr>
        <td><%= image_tag ulist.get_profile_image(100,100) %></td>
        <td><%= ulist.name %></td>
        <td><%= link_to 'Show', user_path(ulist) %></td>
      </tr>
    <% end %>
  </tbody>
</table>